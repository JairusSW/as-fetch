  },
  "homepage": "https://github.com/aspkgy/fetch#readme",
  "devDependencies": {
    "@assemblyscript/loader": "^0.18.29",
    "assemblyscript": "^0.19.7"
  },
  "dependencies": {
    "@assemblyscript/loader": "^0.19.7",
    "cross-fetch": "^3.1.4"
  },
  "peerDependencies": {
