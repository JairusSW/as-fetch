{
  "name": "as-fetch",
  "version": "2.0.0",
  "description": "Fetch API brought to AssemblyScript",
  "scripts": {
    "test": "node tests",
    "asbuild:untouched": "asc assembly/test.ts --target debug --exportRuntime --exportTable --explicitStart",
    "asbuild:optimized": "asc assembly/test.ts --target release --exportRuntime --exportTable --explicitStart",
    "asbuild": "npm run asbuild:untouched && npm run asbuild:optimized"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JairusSW/as-fetch.git"
  },
  "keywords": [
    "assemblyscript",
    "fetch",
    "http",
    "client",
    "request"
  ],
  "author": "Jairus Tanaka",
  "contributors": [
    "Aaron Turner",
    "Joe Pea (trusktr)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JairusSW/as-fetch/issues"
  },
  "homepage": "https://github.com/JairusSW/as-fetch#readme",
  "devDependencies": {
    "assemblyscript": "^0.27.0"
  }
}
