{
  "name": "as-fetch",
  "version": "1.1.0",
  "description": "Fetch API brought to AssemblyScript",
  "main": "imports.js",
  "ascMain": "assembly/fetch.ts",
  "types": "assembly/fetch.ts",
  "scripts": {
    "test": "node tests",
